<%= form_for(@preference, url: admin_access_path(@preference)) do |f| %>
  <%= f.label :artist_sort_order, "Artist Sort Order" %> <br>
  <%= f.radio_button :artist_sort_order, "DESC" %>
  <%=f.label :allow_create_artits, value: "DESC" %><br>
  <%=f.radio_button :artist_sort_order, "ASC"%>
  <%=f.label :artist_sort_order, value: "ASC" %> <br><br>

  <%= f.label :song_sort_order, "Song Sort Order" %> <br>
  <%=f.radio_button :song_sort_order, "DESC" %> 
  <%=f.label :song_sort_order, value: "DESC"%><br>
  <%= f.radio_button :song_sort_order, "ASC" %> 
  <%= f.label :song_sort_order, value: "ASC"%><br>
  <%=f.submit "Submit"%> 
<% end %>